{
  "name": "cvuploader",
  "private": true,
  "version": "0.0.1",
  "main": "./build/main.js",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build:clean": "rm -rf ./build",
    "build:main": "tsc --build ./electron",
    "build:env": "cp -av .env ./build",
    "build:renderer": "vite build",
    "build": "run-s build:clean build:main build:renderer build:env",
    "preview": "vite preview",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./electron/main.ts",
    "package": "yarn build && electron-builder -mw --publish never",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "dotenv": "^16.0.3",
    "electron-log": "^4.4.8",
    "electron-progressbar": "^2.0.1",
    "electron-updater": "^5.3.0",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@types/electron-progressbar": "^1.2.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/source-map-support": "^0.5.6",
    "@vitejs/plugin-react": "^3.0.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "build": {
    "productName": "cvuploader",
    "appId": "com.vatech.cvuploader",
    "files": [
      "./build",
      ".env"
    ],
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "mac": {
      "category": "public.app-category.productivity",
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "electron/resources/entitlements.mac.plist",
      "entitlementsInherit": "electron/resources/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "./build",
      "output": "./release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy-ho/cvuploader.git"
  },
  "author": {
    "name": "Kangju Heo",
    "email": "kj2693119@gmail.com"
  },
  "contributors": [
    {
      "name": "Kangju Heo",
      "email": "kj2693119@gmail.com",
      "url": "https://github.com/cozy-ho"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cozy-ho/cvuploader"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ]
}
