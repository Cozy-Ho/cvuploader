{
  "name": "cvuploader",
  "version": "0.1.0",
  "private": true,
  "main": "build/main.js",
  "scripts": {
    "test": "jest",
    "dev": "next dev renderer",
    "build:clean": "rm -rf ./build",
    "build:main": "tsc --build ./electron",
    "build:env": "cp -av .env ./build",
    "build:renderer": "next build renderer",
    "build": "run-s build:clean build:main build:renderer build:env",
    "start": "next start renderer",
    "start:renderer": "next dev -p 9702",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./electron/main.ts",
    "package": "yarn build && electron-builder -m",
    "postinstall": "electron-builder install-app-deps",
    "lint": "next lint renderer"
  },
  "packageManager": "yarn@3.6.3",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-next": "^3.1.5",
    "electron-progressbar": "^2.1.0",
    "electron-updater": "^6.1.4",
    "minio": "^7.1.3",
    "next": "13.4.19",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.15.0",
    "regenerator-runtime": "^0.14.0"
  },
  "devDependencies": {
    "@types/electron-progressbar": "^1.2.3",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/source-map-support": "^0.5.6",
    "@types/uuid": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "build": {
    "productName": "cvuploader",
    "appId": "com.vatech.cvuploader",
    "publish": {
      "provider": "github"
    },
    "files": [
      "build",
      "renderer/out"
    ],
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "mac": {
      "category": "public.app-category.productivity",
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "electron/resources/entitlements.mac.plist",
      "entitlementsInherit": "electron/resources/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "./release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy-ho/cvuploader.git"
  },
  "author": {
    "name": "Kangju Heo",
    "email": "kj2693119@gmail.com"
  },
  "contributors": [
    {
      "name": "Kangju Heo",
      "email": "kj2693119@gmail.com",
      "url": "https://github.com/cozy-ho"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cozy-ho/cvuploader"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ]
}
